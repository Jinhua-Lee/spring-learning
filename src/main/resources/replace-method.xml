<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="beanA" class="cn.spring.learning.beans.bean.method.replace.ReplaceableMethodMain$ReplaceMethodBean">
        <replaced-method name="method" replacer="myMethodReplacer"/>
    </bean>

    <bean id="myMethodReplacer" class="cn.spring.learning.beans.bean.method.replace.ReplaceableMethodMain.MyMethodReplacer"/>
</beans>